{
  "name": "inhabit-widget-info-chrome",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArkadiumInc/inhabit-widget-info-chrome.git"
  },
  "scripts": {
    "start": "concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "typings": "typings",
    "postinstall": "typings install",
    "build": "npm run clean && npm run tsc",
    "build_prod": "npm run build && browserify -s main dist/main.js > dist/bundle.js && npm run minify",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js"
  },
  "license": "ISC",
  "dependencies": {
    "@types/lodash": "^4.14.40",
    "angular2": "2.0.0-beta.17",
    "lodash": "^4.17.2",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.22",
    "zone.js": "^0.6.12sta"
  },
  "devDependencies": {
    "@types/chrome": "0.0.35",
    "@types/filesystem": "0.0.28",
    "@types/webrtc": "0.0.21",
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^2.0.0",
    "typings": "^0.6.8"
  }
}
