{
  "project": {
    "version": "1.0.0-beta.22-1",
    "name": "inhabit-widget-info-chrome"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "page_scripts/devtools/devtools.js",
        "page_scripts/page_injectables/pageEventHandlers.js",
        "page_scripts/page_injectables/pageResultsRetriever.js",
        "devtools.html",
        "icon16.png",
        "icon48.png",
        "icon128.png",
        "manifest.json",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.css"
      ],
      "scripts": [],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
